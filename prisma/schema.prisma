generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Post {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  authorId String @db.ObjectId
  body     String
  slug     String
  title    String
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
  name  String
}
